<h1>Tests</h1>
<table>
  <thead>
    <tr>
      <th>Title:</th>
      <th>Level:</th>
    </tr>
  </thead>
  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.title %></td>
        <td><%= test.level %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<p>
  <%= link_to 'Create new test', new_test_path %>
</p>

