<h1>Questions for test "<%= @test.title %>"</h1>
<table>
  <thead>
    <tr>
      <th>Title:</th>
      <th>Actions:</th>
    </tr>
  </thead>
  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.body %></td>
        <td><%= link_to 'Delete question', question, method: :delete, data: { confirm: 'Delete question?' } %></td>
    <% end %>
  </tbody>
</table>
<p>
  <%= link_to 'Create new question', new_test_question_path(@test) %>
</p>
