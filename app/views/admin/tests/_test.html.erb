<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.category.title %></td>
  <td><%= questions_count(test) %></td>
  <td><%= test.author.name %></td>
  <td><%= link_to 'Show', admin_test_path(test) %></td>
  <td><%= link_to 'Edit', edit_admin_test_path(test) %></td>
  <td><%= link_to 'Delete', admin_test_path(test), method: :delete,
                  data: { confirm: 'Sure to delete test?' } %></td>
</tr>
