<%= form_with(url: sessions_path, local: true) do |f| %>
  <%= render 'shared/flash' %>

  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email, id: :user_email %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password, id: :user_password %>
  </div>

  <div class="actions">
    <%= f.submit 'Enter' %>
  </div>
<% end %>
